Host *
	ForwardAgent yes
	ForwardX11 yes
	ForwardX11Trusted yes
	CheckHostIP yes
	NoHostAuthenticationForLocalhost yes
	IdentityFile ~/.ssh/id_rsa
	SendEnv LANG LC_*
	HashKnownHosts no
	Compression yes
	ServerAliveInterval 30

host 192.168.*
	user shish
	controlmaster auto
	controlpath ~/.ssh/mux-%r@%h:%p
	controlpersist 3600

host *.shishnet.org
	user shish
	controlmaster auto
	controlpath ~/.ssh/mux-%r@%h:%p
	controlpersist 3600

host *.paheal.net
	user shish
	controlmaster auto
	controlpath ~/.ssh/mux-%r@%h:%p
	controlpersist 3600

Host *.onion
	User shish
	ProxyCommand ncat --proxy 127.0.0.1:9050 --proxy-type socks5 %h %p
