Host *
	ForwardAgent yes
	ForwardX11 yes
	ForwardX11Trusted yes
	CheckHostIP yes
	NoHostAuthenticationForLocalhost yes
	# IdentityFile ~/.ssh/id_rsa
	SendEnv LANG LC_*
	HashKnownHosts no
	Compression yes
	ServerAliveInterval 30
	ControlMaster auto
	#ControlPath ~/.ssh/mux-%r@%h:%p
	ControlPath ~/.ssh/mux-%C
	ControlPersist 3600

Host chai
	User shish
	Hostname 192.168.1.58

Host 192.168.1.38
	User root
	ForwardAgent yes

Host 192.168.1.40
	User magiceraser
	ForwardAgent yes

Host 192.168.1.42
	User magiceraser
	ForwardAgent yes

Host 192.168.*
	User shish

Host shishnet.org
	User shish

Host *.shishnet.org
	User shish

Host *.paheal.net
	User shish
	AddressFamily inet

Host *.onion
	ControlMaster auto
	ControlPath ~/.ssh/mux-%C
	User shish
	ProxyCommand ncat --proxy 127.0.0.1:9050 --proxy-type socks5 %h %p
	HashKnownHosts yes
